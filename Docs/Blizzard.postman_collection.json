		{
			"name": "Auth",
			"item": [
				{
					"name": "Token - Run First",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var data = JSON.parse(responseBody);\r",
									"postman.clearEnvironmentVariable(\"BearerToken\");\r",
									"postman.setEnvironmentVariable(\"BearerToken\", data.access_token);\r",
									"\r",
									"pm.test('Status code is 200', function() {\r",
									"    pm.response.to.have.status(200);\r",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"url": {
							"raw": "https://oauth.battle.net/token?grant_type=client_credentials",
							"protocol": "https",
							"host": [
								"oauth",
								"battle",
								"net"
							],
							"path": [
								"token"
							],
							"query": [
								{
									"key": "grant_type",
									"value": "client_credentials"
								}
							]
						}
					},
					"response": []
				},
